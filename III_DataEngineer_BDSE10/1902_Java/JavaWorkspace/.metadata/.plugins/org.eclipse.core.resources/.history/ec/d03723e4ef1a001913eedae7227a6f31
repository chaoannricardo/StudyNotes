import java.sql.Date;

public class TestFor20190118 {

	public static void main(String[] args) {
		//for迴圈
		for (int i = 0; i <= 5; i++) {
			System.out.println("你這傢伙是不是個白癡阿？ " + "X " + i);
		}
		
		//計算1加到100的總和
		int sum = 0;
		for (int i = 0; i <= 100; i++) {
			sum = sum + i;
		}
		System.out.println("1加到100的總和是" + sum);
		
		
		int sum0 = 0;
		for (int i = 1; i <= 100; i++) {
			sum0 = 1 + i * 2;
		}
		System.out.println(sum0);
	
	}

}

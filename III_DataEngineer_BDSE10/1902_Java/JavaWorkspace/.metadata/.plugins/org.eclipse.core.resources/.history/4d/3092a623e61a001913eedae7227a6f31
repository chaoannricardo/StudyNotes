
public class TestIf20190118 {

	public static void main(String[] args) {
		
		//if else statement
		int age = 15 ;
		if(age <= 10) {System.out.println("不合法");
		} else if (age <= 23 && age > 10) {System.out.println("死學生");	
		} else if (age <= 40 && age > 23) {System.out.println("成年人");
		} else if (age > 65) {System.out.println("老人");
		} else {System.out.println("甚麼也不是");
		}
		
		int student = 35;
		if (student < 20) {System.out.println("教室201");			
		} else if (student >=20 || student < 30) {System.out.println("教室305");
		} else { System.out.println("教室204");			
		}
		
		
		
		
		
		
		
		
		
	}

}

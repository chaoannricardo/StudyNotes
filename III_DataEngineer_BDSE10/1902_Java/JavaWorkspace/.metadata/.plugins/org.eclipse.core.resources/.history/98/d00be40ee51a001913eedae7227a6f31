
public class TestIf20190118 {

	public static void main(String[] args) {
		
		
		int age = 15 ;
		
		if(age <= 10) {System.out.println("不合法");
		} else if (age < 18 && age > 10) {System.out.println("死學生");	
		} else if (age < 40) {System.out.println("成年人");
		} else if (age > 65) {System.out.println("老人");
		} else {System.out.println("甚麼也不是");
		}
		
	}

}
